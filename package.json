{
  "private": true,
  "prisma": {
    "seed": "ts-node ./prisma/seed.mjs"
  },
  "scripts": {
    "build": "prisma generate && nuxt build",
    "dev": "npm run db && npx prisma generate && nuxt dev",
    "dev:recreate": "npm run db && npm run db:recreate && npx prisma generate && nuxt dev",
    "generate": "nuxt generate",
    "db": "docker-compose up -d db",
    "db:stop": "docker-compose down",
    "db:push": "npx prisma db push",
    "db:seed": "npx prisma db seed",
    "db:reset": "npx prisma db push --force-reset",
    "db:recreate": "npm run db:reset && npm run db:seed",
    "db:studio": "npx prisma studio",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint --max-warnings 0 .",
    "typecheck": "nuxt typecheck",
    "start": "NODE_ENV=production node .output/server/index.mjs"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@huntersofbook/naive-ui-nuxt": "^0.7.1",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@nuxtjs/tailwindcss": "^6.4.0",
    "@sidebase/nuxt-auth": "^0.4.3",
    "@types/node": "^18.11.18",
    "eslint": "^8.33.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "nuxt": "^3.2.3",
    "prisma": "^4.11.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vue-tsc": "^1.0.24"
  },
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "@trpc/client": "^10.12.0",
    "@trpc/server": "^10.12.0",
    "next-auth": "^4.18.8",
    "superjson": "^1.12.2",
    "trpc-nuxt": "^0.7.0",
    "zod": "^3.21.2"
  }
}
